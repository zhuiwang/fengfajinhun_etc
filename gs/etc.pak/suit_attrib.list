# 炼化成套装增加的属性及其增加的百分比和波动

# 目前
# 后缀为“金”的套装增加法术攻击。
# 后缀为“木”的套装增加气血上限。
# 后缀为“水”的套装增加防御。
# 后缀为“火”的套装增加速度。
# 后缀为“土”的套装增加物理攻击。

#格式
#attrib$: ({ 属性名,最小等级,最大等级, 最小值, 平均值, 最大值 })

[金]
attrib$ : ({ "mag_power", 70, 180, 18, 18, 18 })

[木]
attrib$ : ({ "max_life",  70, 149, 3600, 3600, 3600 })
attrib$ : ({ "max_life",  150, 180, 4300, 4300, 4300 })

[水]
attrib$ : ({ "def",       70, 149, 1400, 1400, 1400 })
attrib$ : ({ "def",       150, 180, 1700, 1700, 1700 })

[火]
attrib$ : ({ "speed",     70, 180, 41, 41, 41 })

[土]
attrib$ : ({ "phy_power", 70, 180, 32, 32, 32 })
